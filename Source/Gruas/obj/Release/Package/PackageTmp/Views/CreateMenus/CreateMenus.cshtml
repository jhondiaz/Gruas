
@{
    ViewBag.Title = "CreateMenus";
}

<div class="widget-content nopadding">
    <form ng-submit="" class="form-horizontal">

        <div style="width:100%;background-color:#003e65;height:25px;margin-bottom:10px;color:white;text-align:center;font-weight:bold;padding-top:5px;">
            REGISTRAR MENÚ
        </div>

        <div class="row-fluid">
            <div class="span3">
                <label class="control-label">Tipo de Menú:</label>
            </div>
            <div class="span9" style="display:flex;">
                <label style="">Menú Principal</label><input style="margin-left:5px" id="rdmenu" ng-click="rdmenu()" type="radio" name="name" value="" />
                <label style="margin-left:20px;">Menú Secundario</label><input style="margin-left:5px" id="rdsubmenu" ng-click="rdsubmenu()" type="radio" name="name" value="" />
            </div>
        </div>

        <div class="row-fluid">
            <div class="span3">
                <label class="control-label">Nombre Menú:</label>
            </div>
            <div class="span9">
                <input type="text" class="span12" ng-model="name" ng-disabled="isDisabled" placeholder="Nombre Menú" />
            </div>
        </div>

        <div class="row-fluid">
            <div class="span3">
                <label class="control-label">Orden Menú:</label>
            </div>

            <div class="span9">
                <input type="text" class="span12" ng-model="orden" maxlength="50" ng-disabled="isDisabled" placeholder="Orden Menu" onlyletters />
            </div>
        </div>

        <div class="row-fluid">
            <div class="span3">
                <label class="control-label">Icono:</label>
            </div>
            <div class="span9">
                <input type="text" class="span12" ng-model="icono" ng-disabled="true" placeholder="Icono" /><span class="add-on" title="Buscar" ng-disabled="isDisabled" ng-click="openmodaldevices()"><i ng-click="openmodaldevices()" ng-disabled="isDisabled" style="cursor:pointer" class="icon-search"></i></span>
            </div>
        </div>

        <div class="row-fluid submenudis">
            <div class="span3">
                <label class="control-label">Menú:</label>
            </div>
            <div class="span9">
                <select class="span12" ng-model="menua" ng-disabled="isDisabled">
                    <option value="-1">Seleccione</option>
                    <option ng-repeat="itm in menus" value="{{itm.Id}}">{{itm.Menu}}</option>
                </select>
            </div>
        </div>

        <div class="row-fluid submenudis">
            <div class="span3">
                <label class="control-label">Modulo:</label>
            </div>
            <div class="span9">
                <input type="text" class="span12" ng-model="modulo" maxlength="50" ng-disabled="isDisabled" placeholder="Modulo" onlyletters />
            </div>
        </div>

        <div class="row-fluid submenudis">
            <div class="span3">
                <label class="control-label">Ruta:</label>
            </div>
            <div class="span9">
                <input type="text" class="span12" ng-model="ruta" maxlength="50" ng-disabled="isDisabled" placeholder="Ruta" onlyletters />
            </div>
        </div>
        <div class="row-fluid submenudis">
            <div class="span3">
                <label class="control-label">Controlador:</label>
            </div>
            <div class="span9">
                <input type="text" class="span12" ng-model="Controlador" maxlength="50" ng-disabled="isDisabled" placeholder="Controlador" onlyletters />
            </div>
        </div>


        <div style="margin-right:auto;margin-left:auto;text-align:center">
            <button ng-click="validar()" ng-disabled="isDisabled" style="width:200px" class="btn btn-success">Guardar</button>
        </div>

