    <div style="width:100%;background-color:#003e65;height:25px;margin-bottom:10px;color:white;text-align:center;font-weight:bold;padding-top:5px;margin-top:50px;">
        Listado de Menús
    </div>

    <table cellpadding="0" cellspacing="0" border="0" class="table table-bordered" style="max-width:100%;margin-left:auto;margin-right:auto;margin-bottom:10px;" id="editable">
        <thead>
            <tr style="background:#31424a">
                <th class="center">TIPO</th>
                <th class="center">MENÚ</th>
                <th class="center">ORDEN</th>
                <th class="center">ICONO</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="itm in menusall">
                <td style="text-align:center" ng-show="itm.Menu != null">P</td>
                <td style="text-align:center" ng-show="itm.SubMenu != null">S</td>
                <td>{{itm.Menu}}{{itm.SubMenu}}</td>
                <td style="text-align:center">{{itm.OrderMenu}}</td>
                <td style="text-align:center"><i class="{{itm.Icon}}" /></td>
            </tr>
        </tbody>
    </table>
</div>


<style>
    .submenudis {
        display: none;
    }

    i {
        cursor: pointer;
    }


    .form-horizontal .controls {
        margin-left: 0px !important;
    }


    #myTable {
        border-collapse: collapse;
        width: 100%;
        border: 1px solid #ddd;
        font-size: 18px;
    }

        #myTable th, #myTable td {
            text-align: left;
            padding: 12px;
        }

        #myTable tr {
            border-bottom: 1px solid #ddd;
