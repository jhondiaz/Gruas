
@{
    ViewBag.Title = "SolicitudGruas";
}


<div class="widget-content nopadding">
    <div class="form-horizontal">
        <div class="titleforms">
            Solicitud de Grúas
        </div>

        <div class="row-fluid row-padding">
            <div class="span6">
                <div class="span12 titlecampos">
                    <label>Placa Agente</label>
                </div>
                <div class="span12 titlecamposiz">
                    <div class="span2">
                        <div class="span8">
                            <label style="margin-right:5px;">Cedula</label>
                        </div>
                        <div class="span4">
                            <input style="margin-top:10px;" id="rdced" type="radio" name="name" value="" />
                        </div>
                    </div>
                    <div class="span2">
                        <div class="span8">
                            <label style="margin-right:10px;">Placa</label>
                        </div>
                        <div class="span4">
                            <input style="margin-top:10px;" id="rdpl" type="radio" name="name" value="" />
                        </div>
                    </div>
                    <div class="span8">
                        <input type="text" class="span12" ng-model="fl.placasearch" id="placasearch" ng-blur="busplacaag()" placeholder="Seleccione un Agente" /><span class="add-on" style="font-size: 8px !important;" title="Buscar Agente" ng-disabled="isDisabled" ng-click="busplacaag()"><i ng-click="busplacaag()" ng-disabled="isDisabled" style="cursor:pointer" class="icon-search"></i></span>
                    </div>
                </div>
            </div>
            <div class="span6">
                <div class="span12 titlecamposderecha">
                    <label>Nombre del Agente</label>
                </div>
                <div class="span12 titlecamposderechaw">
                    <label ng-model="fl.agensearch" id="agensearch">----</label>
                    @*<input type="text" class="span11"  id="agensearch" capitalize placeholder="Nombre del Agente" />*@
                </div>
            </div>
        </div>

        <div class="row-fluid row-padding">
            <div class="span6">
                <div class="span12 titlecampos">
                    <label>Tipo de servicio de traslado</label>
                </div>
                <div class="span12 titlecamposiz" style="height:40px;">
                    @*<label ng-model="fl.tservicio" ng-repeat="itm in T_S_Translados">{{itm.Nombre}}</label>*@
                    <select class="span11" ng-model="fl.tservicio" ng-change="SearchTr()">
                        <option value="-1">Seleccione</option>
                        <option ng-repeat="itm in T_S_Translados" value="{{itm.Id}}">{{itm.Nombre}}</option>
                    </select>
                </div>

                <div class="span12 titlecampos">
                    <label>Código de Infracción</label>
                </div>
                <div class="span12 titlecamposiz">
                    <input type="text" class="span11" ng-model="fl.coninf" ng-disabled="CusaDis" id="coninf" maxlength="8" capitalize placeholder="Código de la Infracción" />
                </div>
            </div>

            <div class="span6">
                <div class="span12 titlecamposderecha">
                    <label>Causa de la inmovilización</label>
                </div>
                <div class="span12 titlecamposderechaw">
                    <select class="span11" ng-model="fl.causain">
                        <option value="-1">Seleccione</option>
                        <option ng-repeat="itm in C_Inmovilizaciones" value="{{itm.Id}}">{{itm.Nombre}}</option>
                    </select>
                </div>

                <div class="span12 titlecamposderecha">
                    <label>Localidad</label>
                </div>
                <div class="span12 titlecamposderechaw">
                    <input type="text" class="span11" ng-model="fl.localidad" id="localidad" disabled capitalize placeholder="Localidad" />
                    @*<select class="span11" ng-model="fl.localidad">
                            <option value="-1">Seleccione</option>
                            <option ng-repeat="itm in Localidades" name="{{itm.Nombre}}" id="loc" idloc="{{itm.Id}}" value="{{itm.Nombre}}">{{itm.Nombre}}</option>
                        </select>*@
                </div>
            </div>
        </div>


        <div class="row-fluid row-padding" />
        <div class="span12 titlecampos" style="width:98%;">
            <label>Dirección</label>
        </div>
        <div class="span12 titlecamposiz" style="height:40px;">
            <input type="text" class="span11" ng-model="fl.dirr" ng-blur="openmodemap(fl.dirr)" id="drr" capitalize placeholder="Dirección" />@*<span class="add-on" style="font-size: 8px !important;" title="Buscar" ng-disabled="isDisabled" ng-click="openmodemap(fl.dirr)"><i class="icon-map-marker"></i></span>*@
        </div>
    </div>

    <div class="row-fluid row-padding">
        <div class="span6">


            <div class="span12 titlecampos" style="display:none">
                <label>Barrio</label>
            </div>
            <div class="span12 titlecamposiz" style="height:40px;display:none">
                <input type="text" class="span11" ng-model="fl.brr" id="brr" disabled capitalize placeholder="Barrio" />
            </div>


            <div class="span12 titlecampos">
                <label>Tipo de vehículo a Inmovilizar</label>
            </div>
            <div class="span12 titlecamposiz">
                <select class="span11" ng-model="fl.tvehiculo" id="tvlist" ng-disabled="CusaDis">
                    <option value="-1">Seleccione</option>
                    <option ng-repeat="itm in TVehiculoInmovilizars" value="{{itm.Id}}">{{itm.Nombre}}</option>
                </select>
            </div>
        </div>

        <div class="span6">

            <div class="span12 titlecamposderecha">
                <label>Cantidad Vehículos</label>
            </div>
            <div class="span12 titlecamposderechaw" style="height:40px;">
                <div class="span10">
                    <select class="span11" ng-model="fl.cnvhi" id="numgruas" ng-disabled="CusaDis">
                        <option value="-1">Seleccione</option>
                    </select>
                </div>
                <div class="span2">
                    @*<i class="fa fa-plus-square" ng-click="addvehi" style="color:#009fe3;font-size:37px;"></i>*@
                    <input type="button" class="btn btn-success" ng-click="addvehi()" ng-disabled="CusaDis" name="name" value="Agregar" />
                </div>
            </div>

            @*<div class="span12 titlecampos">
                    <label>Localidad</label>
                </div>
                <div class="span12 titlecamposiz">
                    <input type="text" class="span11" ng-model="fl.loc" capitalize placeholder="Localidad" />
                </div>*@

            @*<div class="span12 titlecamposderecha">
                    <label>Tipo de Grúa</label>
                </div>
                <div class="span12 titlecamposderechaw">
                    <select class="span11" ng-model="fl.tgrua">
                        <option value="-1">Seleccione</option>
                        <option ng-repeat="itm in TipoGruas" value="{{itm.Id}}">{{itm.Nombre}}</option>
                    </select>
                </div>*@



            @*<div class="span12 titlecamposderecha">
                    <label>Cantidad De Grúas</label>
                </div>
                <div class="span12 titlecamposderechaw">
                    <select class="span11" ng-model="fl.numgruas" id="numgruas">
                        <option value="-1">Seleccione</option>
                    </select>
                </div>*@
        </div>

        <div class="span12">
            
            <table cellpadding="0" cellspacing="0" border="0" class="table table-bordered" style="max-width:60%;margin-left:auto;margin-right:auto;margin-bottom:10px;" id="editable">
                <thead>
                    <tr style="background:#31424a;">
                        <th style='color:#16b8ea;' class="center">Tipo de Vehículo a Inmovilizar</th>
                        <th style='color:#16b8ea;' class="center">Cantidad </th>
                        <th style='color:#16b8ea;' class="center" colspan="2">Accion</th>
                    </tr>
                </thead>
                <tbody id="add"></tbody>
            </table>

        </div>

    </div>
    <div class="map">
        <div id="floating-panel">
            <input id="address" type="text">
            <input id="submit" class="btn btn-success btncl" type="button" value="Buscar">
        </div>
        <div id="map" style="height:60vh;width:60vw;margin-right:auto;margin-left:auto;margin-top:5%;"></div>
        <div id="latlong" style="display:none"></div>

        <div style="height:45px;width:60vw;margin-right:auto;margin-left:auto;background-color:rgba(128, 128, 128,0.7);">

            <input class="btn btn-success" type="button" ng-click="closemap()" style="float:right;width:150px; background:#e6e6e6;color:black; margin-left:30px;margin-right:30px;" value="Cerrar">
        </div>
    </div>

    <div class="row-fluid row-padding" style="text-align:center;margin-bottom:15px;">
        <input style="width:200px;" type="button" class="btn btn-success" ng-click="validarcorr(fl.dirr)" ng-disabled="disbtn" id="btndissend" name="name" value="Enviar" />
    </div>
</div>


<div id="asd">

</div>

<style>
    #floating-panel {
        position: absolute;
        margin-top: 5%;
        margin-left: 30%;
        z-index: 5;
        background-color: #fff;
        padding: 5px;
        border: 1px solid #999;
        text-align: center;
        font-family: 'Roboto','sans-serif';
        line-height: 30px;
        padding-left: 10px;
    }


    .titleforms {
        width: 100%;
        background-color: #003e65;
        height: 25px;
        margin-bottom: 10px;
        color: white;
        text-align: center;
        font-weight: bold;
    }

    .titlecampos {
        background-color: #DBF4FA;
        text-align: center;
        color: #16b8ea;
    }

    .titlecamposderecha {
        background-color: #DBF4FA;
        border-left: 1px solid #16b8ea;
        text-align: center;
        color: #16b8ea;
    }

    .titlecamposiz {
        text-align: center;
        min-height: 40px;
    }

    .titlecamposderechaw {
        border-left: 1px solid #16b8ea;
        text-align: center;
        height: 40px;
    }

    .row-padding {
        padding-left: 13px;
    }

    .widget-content {
        height: auto !important;
    }

    input {
        margin-top: 5px;
    }

    select {
        margin-top: 5px;
    }

    label {
        margin-top: 5px;
    }


    .easy-autocomplete {
        width: 100% !important;
    }

    .map {
        height: 77vh;
        width: 98%;
        position: absolute;
        top: 15px;
        display: none;
        margin-left: auto;
        margin-right: auto;
        left: 10px;
        background-color: rgba(128, 128, 128,0.5)
    }
</style>
